        "plat/[d-x]*": true,
        "plat/a[a-q]*": true,
        "plat/aspeed": true,

        "plat/arm/board/[a-b]*": true,
        "plat/arm/board/[d-z]*": true,
        "plat/arm/board/corst*": true,
        "plat/arm/board/fvp*": true,

        "plat/arm/board/common/[c-z]*": true,

